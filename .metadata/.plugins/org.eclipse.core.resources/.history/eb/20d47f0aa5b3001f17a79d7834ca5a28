package ProblemSolving;

import java.util.Iterator;

public class oneTwoThreePattern {

	public static void main(String[] args) {

		for (int i = 0; i < 6; i++) {
			int k = i + 1;
			for (int j = i; j < 6; j++) {
				System.out.print(" ");
			}
			for (int j = 0; j < k; j++) {

				i = j;
				System.out.print(i + " ");

			}

			System.out.println();

		}System.out.println();

		
		
		System.out.println("next Start pattern");
		
		for(int i=0;i<6;i++){
			int z=0;
			for(int j=0;j<z;j++){
				
				if(z<6){
					System.out.print("*");
					
					if(i>=2){
						z+=2;
					}
					else{
						z+=1;
					}
				}
				
				
			}
			System.out.println();
		}
	}

}
