package ProblemSolving;

import java.util.Iterator;

public class oneTwoThreePattern {

	public static void main(String[] args) {

		for (int i = 0; i < 6; i++) {
			int k = i + 1;
			for (int j = i; j < 6; j++) {
				System.out.print(" ");
			}
			for (int j = 0; j < k; j++) {

				i = j;
				System.out.print(i + " ");

			}

			System.out.println();

		}System.out.println();

		
		
		System.out.println("next Start pattern");
		int z=1;
		for(int i=0;i<6;i++){
			
			for(int j=0;j<z;j++){
				
				System.out.print("*");
					
			}
			if(i<1){
				z+=1;
			}else{
				z+=2;
			}
			System.out.println();
			
		}
	}

}
